.picWacher{
  width: 100%;
  box-sizing: border-box;
  padding: 10rpx;
}

.picWacher .pic{
  height: 600rpx;
}
.picWacher .pic image{
  width: 100%;
  height: 100%;
}

.picWacher .pic-picker{
  height: 250rpx;
}

.picWacher .pic-picker .inner{
  display: inline-block;
  /* flex-direction: row; */
  height: 160rpx;
  width: 150rpx;
  position: relative;
  padding-top: 40rpx;  
  margin: 0 10rpx;
} 

.picWacher .pic-picker .inner .team-item {
  display: flex;
  flex-direction: column;
  width: 150rpx;
  height: 260rpx;
  z-index: 9;
} 
.picWacher .pic-picker .inner .team-item image{
  width: 150rpx;
  height: 160rpx;
}
.picWacher .pic-picker .inner .ico{
  font-size: 40rpx;
  position: absolute;
  color: red;
  top:-5rpx;
  left:70rpx;
  z-index: 999;
}
.active{
  border: 2px solid red;
}

